{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "testMatch": [
    "**/test/*.(ts?|tsx?)"
  ],
  "moduleNameMapper": {
    "\\.(css)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": ["./src/setupTests.ts"],
  "transform": {
    "^.+\\.(ts?|tsx?)$": "ts-jest"
  }
}