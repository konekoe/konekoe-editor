{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "testMatch": [
    "**/test/*.test.(ts|tsx)"
  ],
  "moduleNameMapper": {
    "\\.(css)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": ["./src/setupTests.ts"],
  "transform": {
    "^.+\\.test.(ts|tsx)$": "ts-jest"
  }
}